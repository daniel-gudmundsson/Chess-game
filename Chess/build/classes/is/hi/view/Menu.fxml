<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<MenuBar fx:id="menu" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="is.hi.view.MenuController">
  <menus>
    <Menu fx:id="file" mnemonicParsing="false" text="File">
      <items>
            <MenuItem fx:id="newGame" mnemonicParsing="false" onAction="#newGameHandler" text="New game" />
        <MenuItem fx:id="close" mnemonicParsing="false" onAction="#closeHandler" text="Close" />
      </items>
    </Menu>
    <Menu fx:id="help" mnemonicParsing="false" text="Help">
      <items>
        <MenuItem fx:id="about" mnemonicParsing="false" onAction="#aboutHandler" text="About" />
      </items>
    </Menu>
  </menus>
</MenuBar>
